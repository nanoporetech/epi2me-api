{
  "name": "epi2me-api",
  "version": "2.57.1",
  "repository": "https://git.oxfordnanolabs.local/metrichor/api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "private": true,
  "main": "dist/epi2me.js",
  "dependencies": {
    "aws-sdk": "2.292.0",
    "crypto": "1.0.1",
    "fs-extra": "^7.0.0",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "proxy-agent": "3.0.1",
    "queue-async": "git+https://github.com/rmp/queue.git",
    "recursive-readdir": "2.2.2",
    "request": "2.88.0",
    "request-progress": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "5.3.0",
    "eslint-config-defaults": "9.0.0",
    "istanbul": "^0.4.5",
    "memwatch-ng": "^1.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "proxyquire": "2.0.1",
    "sinon": "6.1.5",
    "tmp": "0.0.33",
    "xunit-file": "*"
  },
  "scripts": {
    "lint": "yarn install && eslint --fix lib",
    "test": "yarn lint && mocha --recursive --require babel-register test/epi2me.Spec",
    "test_integration": "yarn istanbul cover mocha --recursive --require babel-register test/e2e",
    "cover": "yarn build && nyc --reporter=html mocha test/epi2me.Spec",
    "dist": "yarn test && yarn babel lib -d dist && cp lib/default_options.json dist/"
  }
}
