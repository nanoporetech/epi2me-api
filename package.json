{
  "name": "@metrichor/epi2me-api",
  "private": "true",
  "version": "6.1.0",
  "license": "MPL-2.0",
  "repository": "git@github.com:nanoporetech/epi2me-api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "author": {
    "name": "Oxford Nanopore Technologies",
    "email": "support@nanoporetech.com",
    "url": "https://nanoporetech.com"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@grpc/grpc-js": "^1.3.7",
    "@improbable-eng/grpc-web": "^0.14.1",
    "@improbable-eng/grpc-web-node-http-transport": "^0.14.1",
    "@types/socket.io-client": "^1.4.36",
    "aws-sdk": "^2.1002.0",
    "axios": "^0.22.0",
    "cross-fetch": "^3.1.5",
    "fdir": "^5.1.0",
    "fs-extra": "^10.0.0",
    "fs-inspect": "^1.0.3",
    "google-protobuf": "^3.18.1",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "immutable": "^4.0.0-rc.15",
    "proxy-agent": "^5.0.0",
    "rxjs": "^7.4.0",
    "socket.io-client": "^2.4.0",
    "ts-runtime-typecheck": "^2.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typed-document-node": "2.2.0",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-generic-sdk": "2.2.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-typed-document-node/core": "3.1.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/chai": "^4.2.22",
    "@types/google-protobuf": "^3.15.5",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.4",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "bunyan": "^1.8.15",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^3.3.7",
    "grpc-tools": "^1.11.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "mocha": "^9.1.2",
    "mock-fs": "^5.1.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^11.1.2",
    "tmp": "^0.2.1",
    "ts-node": "^10.2.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.4.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "postinstall": "npm run graphql && npm run protos || echo \"\\033[33m** Remember to install the protobufs submodule and rebuild them locally **\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint:quiet -- --fix",
    "test": "mocha --recursive --require ts-node/register --extension ts --extension js test/",
    "cover": "npx nyc --reporter=html --reporter=text mocha --recursive --require ts-node/register --extension ts --extension js test/",
    "clean": "rm -rf dist",
    "pregraphql": "rm -f ./src/generated/*",
    "graphql": "graphql-codegen --config .gql-codegen.yml",
    "preprotos": "rm -rf ./protos/*",
    "protos": "sh ./build_protos.sh",
    "prebuild": "npm run clean && npm run graphql && npm run protos",
    "build": "rollup -c",
    "typecheck": "tsc --noEmit --watch",
    "prepare": "husky install"
  }
}
