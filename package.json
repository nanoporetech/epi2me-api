{
  "name": "epi2me-api",
  "version": "2.58.1",
  "license": "MPL-2.0",
  "repository": "https://git.oxfordnanolabs.local/metrichor/api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "aws-sdk": "2.392.0",
    "crypto": "1.0.1",
    "fs-extra": "^7.0.0",
    "lodash": "4.17.11",
    "proxy-agent": "3.0.3",
    "recursive-readdir": "2.2.2",
    "request": "2.88.0",
    "request-progress": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/bunyan": "^1.8.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/prettier": "^1.15.2",
    "@types/terser-webpack-plugin": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "bunyan": "^1.8.12",
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "mocha": "5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.16.0",
    "prettier-eslint": "^8.8.2",
    "proxyquire": "2.1.0",
    "sinon": "7.2.3",
    "terser-webpack-plugin": "^1.2.1",
    "tmp": "0.0.33",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "xunit-file": "*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "lint-js": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "fix-js": "yarn run lint-js --fix",
    "lint": "yarn lint-js",
    "deps": "yarn install",
    "clean:dist": "rm -rf dist",
    "test": "yarn deps && yarn lint && mocha --recursive --require @babel/register test",
    "cover": "yarn deps && yarn lint && yarn nyc --reporter=html --reporter=text mocha --recursive --require @babel/register test",
    "dist": "yarn test && yarn babel lib -d dist && cp lib/default_options.json dist/ && yarn webpack && babel index-src.js -o index.js",
    "build:dist": "yarn clean:dist && yarn webpack-cli --config ./config/webpack.config.js"
  }
}
