{
  "name": "@metrichor/epi2me-api",
  "private": "true",
  "version": "6.1.0",
  "license": "MPL-2.0",
  "repository": "git@github.com:nanoporetech/epi2me-api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "author": {
    "name": "Oxford Nanopore Technologies",
    "email": "support@nanoporetech.com",
    "url": "https://nanoporetech.com"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@types/socket.io-client": "^3.0.0",
    "aws-sdk": "^2.1002.0",
    "axios": "^0.22.0",
    "cross-fetch": "^3.1.5",
    "fs-extra": "^10.0.0",
    "fs-inspect": "^1.0.3",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "immutable": "^4.0.0-rc.15",
    "proxy-agent": "^5.0.0",
    "rxjs": "^7.4.0",
    "socket.io-client": "^4.4.1",
    "ts-runtime-typecheck": "^2.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.8",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/jest": "^27.4.1",
    "@types/sinon": "^10.0.4",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^3.3.7",
    "husky": "^7.0.2",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^11.1.2",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.6.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "postinstall": "npm run graphql",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint:quiet -- --fix",
    "test": "jest --verbose",
    "cover": "jest --verbose --coverage",
    "clean": "rm -rf dist",
    "pregraphql": "rm -f ./src/generated/*",
    "graphql": "graphql-codegen --config .gql-codegen.yml",
    "prebuild": "npm run clean && npm run graphql",
    "build": "rollup -c",
    "typecheck": "tsc --noEmit --watch",
    "prepare": "husky install"
  }
}
